n                        class=\"flex_1\"\r\n                        v-model=\"item.value\"\r\n                        :placeholder=\"`请输入${item.title}`\"\r\n                        clearable>\r\n                    </el-input>\r\n                    <!-- 输入框(手机号) -->\r\n                    <el-input\r\n                        v-if=\"item.type == 'phone'\"\r\n                        size=\"mini\"\r\n                        class=\"flex_1\"\r\n                        v-model=\"item.value\"\r\n                        :placeholder=\"`请输入${item.title}`\"\r\n                        oninput=\"value=value.replace(/[^\\d]/g,'')\"\r\n                        maxlength=\"11\"\r\n                        clearable>\r\n                    </el-input>\r\n                    <!-- 下拉框 -->\r\n                    <el-select\r\n                        :remote=\"item.isRemote==1?true:false\" \r\n                        :remote-method=\"item.isRemote==1?item.remoteMethod:''\"\r\n                        v-if=\"item.type=='select'\" :placeholder=\"`请选择${item.title}`\" v-model=\"item.value\" size=\"mini\" class=\"flex_1\" filterable clearable >\r\n                        <el-option\r\n                            v-for=\"(op_item,op_index) in item.option\"\r\n                            :key=\"op_index\"\r\n                            :label=\"op_item.title\"\r\n                            :value=\"op_item.id\"\r\n                        ></el-option>\r\n                    </el-select>\r\n                    <!-- 日期范围 -->\r\n                    <el-date-picker\r\n                        class='flex_1'\r\n                        v-if=\"item.type == 'dateDaterange'\"\r\n                        v-model=\"item.value\"\r\n                        value-format=\"yyyy-MM-dd\"\r\n                        type=\"daterange\"\r\n                        range-separator=\"-\"\r\n                        start-placeholder=\"开始日期\"\r\n                        end-placeholder=\"结束日期\">\r\n                    </el-date-picker>\r\n                    <!-- 选择日期 -->\r\n                    <el-date-picker\r\n                        class='flex_1'\r\n                        v-if=\"item.type == 'date'\"\r\n                        v-model=\"item.value\"\r\n                        value-format=\"yyyy-MM-dd\"\r\n                        type=\"date\"\r\n                        :placeholder=\"`请选择${item.title}`\">\r\n                    </el-date-picker>\r\n                    <!-- 单选框 -->\r\n                    <div class='flex_1' v-if=\"item.type=='radio'\">\r\n                        <el-radio-group v-model=\"item.value\">\r\n                            <el-radio v-for=\"(op_item,op_index) in item.option\" :key=\"op_index\" :label=\"op_item.id\">{{op_item.title}}</el-radio>\r\n                        </el-radio-group>\r\n                    </div>\r\n            </div>\r\n            <div class=\"flex row search_btn mar_t_20\">\r\n                <el-button type=\"primary\" size=\"mini\" @click=\"submit('search')\">查询</el-button>\r\n                <el-button size=\"mini\" @click=\"submit('reset')\">重置</el-button>\r\n            </div>\r\n        </div>\r\n</template>\r\n\r\n<script>\r\n//type类型  text  date_day 年月日  \r\nexport default {\r\n  props: {\r\n      searchList: {\r\n          type: Array,\r\n          default: () => [],\r\n      }\r\n  },\r\n  name: \"\",\r\n  data() {\r\n    return {\r\n      \r\n    };\r\n  },\r\n  methods: {\r\n    \r\n    submit(type){\r\n        if(type=='reset'){//重置\r\n            this.searchList.map(item=>{\r\n                item.value = '';\r\n            });\r\n        };\r\n        this.$emit('search_list',this.searchList);\r\n    },\r\n\r\n\r\n\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n    .search_btn{\r\n        width:200px;\r\n        height:30px;\r\n        padding:0;\r\n    }\r\n</style>\r\n"]}]}